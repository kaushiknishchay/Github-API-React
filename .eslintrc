{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "rules": {
    "linebreak-style": 0
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ]
}